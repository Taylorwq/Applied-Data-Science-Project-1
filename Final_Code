    "window = 21\n",
    "merged[\"spx_vol_21d\"] = merged[\"spx_ret\"].rolling(window).std()\n",
    "\n",
    "plt.figure()\n",
    "plt.plot(merged[\"date\"], merged[\"spx_vol_21d\"])\n",
    "plt.title(\"21-day rolling volatility of SPX log returns\")\n",
    "plt.xlabel(\"date\")\n",
    "plt.ylabel(\"rolling std\")\n",
